
public class Twentyfive {

	public static void main(String[] args) {
	//*********** 54321012345
	//********* 432101234
	//******* 3210123
	//***** 21012
	//*** 101
	//* 0
	//*** 101 
	//***** 21012 
	//******* 3210123 
	//********* 432101234 
	//*********** 54321012345

	int i, j=0;
	int k;
	
	while(j<=5)
	{
		i = 0;
		k = -2*j+11;
		while(i<=k)
		{
			if(i<k)
				System.out.print("*");	
			else if (i==k)
				System.out.println(" ");	
			i++;
			}
		j++;
		}

	}
  }